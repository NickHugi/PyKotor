{
	"$schema": "vscode://schemas/color-theme",
	"type": "dark",
	"colors": {
		"activityBar.activeBorder": "#414045",
		"activityBar.activeFocusBorder": "#414045",
		"activityBar.activeBackground": "#3031363f",
		"activityBar.background": "#303136",
		"activityBar.border": "#414045",
		"activityBar.dropBorder": "#414045",
		"activityBar.foreground": "#ffffff",
		"activityBar.inactiveForeground": "#ffffff74",
    "activityBarBadge.background": "#199aff",
    "activityBarBadge.foreground": "#ffffff",
    "badge.background": "#199aff",
    "badge.foreground": "#ffffff",
		"button.background": "#1780fa",
		"button.foreground": "#ffffff",
		"button.hoverBackground": "#0a64ca",
		"descriptionForeground": "#cccccc",
		"dropdown.background": "#3c3d3f",
		"dropdown.border": "#ffffff33",
		"dropdown.foreground": "#cccccc",
		"dropdown.listBackground": "#3c3d3f",
		"editor.background": "#242529",
		"editor.findMatchBackground": "#2d2e30",
		"editor.findMatchHighlightBackground": "#505154",
		"editor.findRangeHighlightBackground": "#505154",
		"editor.foreground": "#ffffff",
		"editor.lineHighlightBackground": "#303239",
		"editor.selectionBackground": "#444444",
		"editorGroup.border": "#393a3c",
		"editorGroup.emptyBackground": "#303136",
		"editorGroupHeader.noTabsBackground": "#303136",
		"editorGroupHeader.tabsBackground": "#303136",
		"editorGroupHeader.tabsBorder": "#1a191c",
		"editorWidget.background": "#303136",
		"focusBorder": "#4f4e53",
		"foreground": "#ffffff",
    "input.background": "#414045",
		"input.foreground": "#ffffff",
		"inputOption.activeBackground": "#242529",
		"list.activeSelectionBackground": "#403f44",
		"list.activeSelectionForeground": "#ffffff",
		"list.dropBackground": "#4f5154",
		"list.errorForeground": "#ffffff",
		"list.focusBackground": "#403f44",
		"list.focusForeground": "#ffffff",
		"list.highlightForeground": "#ffffff",
		"list.hoverForeground": "#ffffff",
		"list.inactiveFocusBackground": "#403f44",
		"list.inactiveSelectionBackground": "#403f44",
		"list.inactiveSelectionForeground": "#ffffff",
		"list.invalidItemForeground": "#ffffff",
		"list.warningForeground": "#ffffff",
    "minimap.background": "#00000079",
		"panel.background": "#242529",
		"panel.border": "#4d4d50",
		"selection.background": "#303237",
		"sideBar.background": "#303136",
		"sideBar.border": "#1a191c",
		"sideBar.dropBackground": "#303136",
		"sideBar.foreground": "#eaf0f6",
		"sideBarSectionHeader.background": "#3031367a",
		"sideBarSectionHeader.border": "#303136a0",
		"sideBarSectionHeader.foreground": "#8d8d8d",
		"sideBarTitle.foreground": "#6c6b70",
		"statusBar.background": "#414045",
		"statusBar.border": "#4d4d50",
		"statusBar.debuggingBackground": "#414045",
		"statusBar.debuggingBorder": "#4d4d50",
		"statusBar.debuggingForeground": "#a0a0a0",
		"statusBar.foreground": "#a0a0a0",
		"statusBar.noFolderBackground": "#414045",
		"statusBar.noFolderBorder": "#4d4d50",
		"statusBar.noFolderForeground": "#a0a0a0",
		"statusBarItem.activeBackground": "#414045",
		"statusBarItem.hoverBackground": "#414045",
		"tab.activeBackground": "#414045",
		"tab.activeBorder": "#1a191c",
		"tab.activeBorderTop": "#292a2f",
		"tab.activeForeground": "#f7f7f7",
		"tab.border": "#3d3c40",
		"tab.inactiveBackground": "#303136",
		"tab.inactiveForeground": "#b7b7b7",
    "tab.unfocusedActiveForeground": "#f7f7f7",
		"tab.unfocusedActiveBorder": "#55555b",
		"tab.unfocusedActiveBorderTop": "#55555b",
		"tab.unfocusedInactiveForeground": "#b7b7b7",
		"textBlockQuote.background": "#ff000000",
		"textBlockQuote.border": "#ff000000",
		"textCodeBlock.background": "#ff000000",
		"titleBar.activeBackground": "#414045",
		"titleBar.activeForeground": "#b8b7bc",
		"titleBar.border": "#4d4d50",
		"titleBar.inactiveBackground": "#414045",
		"titleBar.inactiveForeground": "#b8b7bc",
		"widget.shadow": "#222222"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#7f8c98"
			}
		},
		{
			"name": "String",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#fe8170"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"storage.type.function.arrow"
			],
			"settings": {
				"foreground": "#ffffffd8"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"storage.type",
				"storage.modifier",
				"keyword",
				"variable.language.this",
				"variable.language.super",
				"keyword.other.typedef",
				"constant.language"
			],
			"settings": {
				"foreground": "#fe7ab2"
			}
		},
		{
			"name": "ClassDeclName",
			"scope": [
				"entity.name.type.class"
			],
			"settings": {
				"foreground": "#6bdfff"
			}
		},
		{
			"name": "InstanceProperties",
			"scope": [
				"variable.other.property",
				"variable.other.object.property"
			],
			"settings": {
				"foreground": "#b181ec"
			}
		},
		{
			"name": "OtherDeclarations",
			"scope": [
				"entity.name.function",
				"entity.name.function.method"
			],
			"settings": {
				"foreground": "#4db1cb"
			}
		},
		{
			"name": "ModuleNames",
			"scope": [
				"entity.name.type.module",
				"entity.other.inherited-class",
				"entity.name.class"
			],
			"settings": {
				"foreground": "#d9baff"
			}
		},
		{
			"name": "Markup bold",
			"scope": [
				"markup.bold",
				"markdown.bold",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#6bdfff"
			}
		},
		{
			"name": "Markup heading",
			"scope":  [
				"markup.heading",
				"markdown.heading",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#6bdfff"
			}
		},
		{
			"name": "Markup italic",
			"scope":  [
				"markup.italic",
				"markdown.italic",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#d9baff"
			}
		},
		{
			"name": "Markup quote",
			"scope":  [
				"markup.quote",
				"markdown.quote",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#d9baff"
			}
		},
		{
			"name": "TypeAnnotation",
			"scope": [
				"meta.type.annotation"
			],
			"settings": {
				"foreground": "#6bdfff"
			}
		},
		{
			"name": "tag",
			"scope": [
				"punctuation.definition.tag",
				"entity.name.tag",
				"punctuation.separator.key-value"
			],
			"settings": {
				"foreground": "#fe7ab2"

			}
		},
		{
			"name": "JSSeparator",
			"scope": [
				"punctuation.separator.key-value.js"
			],
			"settings": {
				"foreground": "#ffffffd9"
			}
		},
		{
			"name": "Attribute",
			"scope": [
				"meta.attribute",
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#cc9767"
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#d8c87c"
			}
		},
		{
			"name": "Property",
			"scope": [
				"variable.other.property"
			],
			"settings": {
				"foreground": "#abf2e4"
			}
		},
		{
			"name": "Operator",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"foreground": "#ffffffd9"
			}
		},
		{
			"name": "Parameter",
			"scope": [
				"variable.parameter"
			],
			"settings": {
				"foreground": "#ffffffd9"
			}
		},
		{
			"name": "ClassType",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#d9baff"
			}
		},
		{
			"name": "Assignment",
			"scope": [
				"keyword.operator.assignment"
			],
			"settings": {
				"foreground": "#ffffffd9"
			}
		},
		{
			"name": "GQLFragmentName",
			"scope": [
				"entity.name.fragment.graphql",
				"variable.fragment.graphql"
			],
			"settings": {
				"foreground": "#6bdfff"
			}
		},
		{
			"name": "QuasiElement",
			"scope": [
				"punctuation.quasi.element"
			],
			"settings": {
				"foreground": "#ffffffd9"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable.other.readwrite"
			],
			"settings": {
				"foreground": "#ffffffd9"
			}
		},
		{
			"name": "Brace",
			"scope": [
				"meta.brace"
			],
			"settings": {
				"foreground": "#ffffffd9"
			}
		},
		{
			"name": "Arithmetic",
			"scope": [
				"keyword.operator.arithmetic"
			],
			"settings": {
				"foreground": "#b281eb"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#6796e6"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#cd9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#f44747"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#b267e6"
			}
		}
	]
}
