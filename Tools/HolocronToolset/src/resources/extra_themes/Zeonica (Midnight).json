	"tokenColors": [
		{
			"name": "Comments",
			"scope": "comment",
			"settings": {
				"foreground": "#9b9bd4"
			}
		},
		{
			"name": "Parameters",
			"scope": [
				"variable.parameter"
			],
			"settings": {
				"foreground": "#f5f5ff"
			}
		},
		{
			"name": "Classes",
			"scope": [
