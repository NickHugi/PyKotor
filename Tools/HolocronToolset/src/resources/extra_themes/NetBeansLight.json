{
    "type": "light",
    "colors": {
        "editor.background": "#ffffff",
        "editor.foreground": "#000000",
        "editor.selectionBackground": "#b0c5e3",
        "editor.findMatchBackground": "#b0c5e3cc",
        "editor.findMatchHighlightBackground": "#ffb442cc",
        "editor.lineHighlightBackground": "#e9eff8",
        "editorCursor.foreground": "#000000",
        "editorWhitespace.foreground": "#ffffff",
        "editor.wordHighlightBackground": "#eceba3cc",
        "peekViewResult.matchHighlightBackground": "#ffb442cc",
        "editorIndentGuide.background": "#D3D3D3",
        "editorLineNumber.foreground": "#000000"
    },
    "tokenColors": [
        {
            "settings": {
                "foreground": "#000000ff",
                "background": "#ffffffff"
            }
        },
        {
            "name": "Class and Interface Name",
            "scope": "entity.name.type.class, entity.name.type.interface",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#000000"
            }
        },
        {
            "name": "Inherited Class Name",
            "scope": "entity.other.inherited-class",
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "name": "Function Name",
            "scope": "entity.name.function,meta.definition.method.ts entity.name.function.ts",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#000000"
            }
        },
        {
            "name": "Constant Other",
            "scope": "constant.other",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#009900"
            }
        },
        {
            "name": "Comment",
            "scope": "comment,comment.block.documentation.js,comment.line.double-slash.js,entity.name.type.instance.jsdoc,variable.other.jsdoc",
            "settings": {
                "foreground": "#969696"
            }
        },
        {
            "name": "Comment: Annotations",
            "scope": "storage.type.class.jsdoc,keyword.other.documentation",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#969696"
            }
        },
        {
            "name": "Regex",
            "scope": "string.regexp.js",
            "settings": {
                "foreground": "#9933CC"
            }
        },
        {
            "name": "Keywords",
            "scope": "keyword,storage,constant.language,entity.name.tag,punctuation.definition.tagstorage.type.function.js,storage.type.js,variable.language.this,variable.language.java,variable.language.super.ts,keyword.control,keyword.operator.new,keyword.operator.expression.typeof.js,keyword.operator.expression.delete.js,keyword.operator.expression.instanceof.js,keyword.other.import,keyword.other.import.static,keyword.other.package,support.type.primitive.ts",
            "settings": {
                "foreground": "#0000E6"
            }
        },
        {
            "name": "Object Field",
            "scope": "support.type.property-name.json,entity.other.attribute-name,meta.object-literal.key,variable.other.property,variable.other.object.property,variable.other.constant.property.js,variable.other.constant.object.property.js,support.variable.property.dom.js,variable.object.property",
            "settings": {
                "foreground": "#009900"
            }
        },
        {
            "name": "String",
            "scope": "string,punctuation.definition.string.begin.js,punctuation.definition.string.end.js,string.quoted.single.js,string.quoted.double.js",
            "settings": {
                "foreground": "#CE7B00"
            }
        },
        {
            "name": "Separators",
            "scope": "punctation,punctuation.terminator.statement.js,punctuation.separator.key-value.js,punctuation.separator.comma.js,punctuation.definition.block.js,punctuation.accessor.js,punctuation.definition.parameters.begin.js,punctuation.definition.parameters.end.js",
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "name": "Global",
            "scope": "support.variable.dom.ts",
            "settings": {
                "foreground": "#9F0D79"
            }
        },
        {
            "name": "Operators",
            "scope": "keyword.operator,storage.type.function.arrow",
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "name": "Identifier",
            "scope": "variable,meta.function-call.ts entity.name.function.ts,meta.function-call.js entity.name.function.js,support.function,support.variable.property.js,storage.modifier.import,storage.modifier.package,storage.type.annotation,storage.type.generic,storage.type.object.array,storage.type.java",
            "settings": {
                "foreground": "#000000",
                "fontStyle": "plain"
            }
        },
        {
            "name": "Class Name",
            "scope": "support.class.js",
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "name": "HTML and XML: Doctype",
            "scope": "meta.tag.sgml.doctype.html,meta.tag.sgml.doctype.xml,keyword.other.doctype.xml,variable.language.documentroot.xml",
            "settings": {
                "foreground": "#628FB5"
            }
        },
        {
            "name": "HTML and XML: Tags",
            "scope": "entity.name.tag, punctuation.definition.tag.xml, punctuation.definition.tag.html, punctuation.definition.tag.begin.html, punctuation.definition.tag.end.html",
            "settings": {
                "foreground": "#0000E6"
            }
        },
        {
            "name": "CSS: Property Name",
            "scope": "support.type.property-name.css",
            "settings": {
                "foreground": "#0000E6"
            }
        },
        {
            "name": "CSS: Property Value",
            "scope": "keyword.other",
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "name": "CSS: At Rule",
            "scope": "keyword.control.at-rule.keyframes.css",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#000000"
            }
        },
        {
            "scope": "token.info-token",
            "settings": {
                "foreground": "#316bcd"
            }
        },
        {
            "scope": "token.warn-token",
            "settings": {
                "foreground": "#cd9731"
            }
        },
        {
            "scope": "token.error-token",
            "settings": {
                "foreground": "#cd3131"
            }
        },
        {
            "scope": "token.debug-token",
            "settings": {
                "foreground": "#800080"
            }
        }
    ]
}